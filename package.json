{
  "name": "tslint-config-kata",
  "version": "1.0.5",
  "description": "TSLint config used at Kata.ai",
  "main": "tslint.json",
  "repository": {
    "type": "git",
    "url": "git://github.com/kata-ai/tslint-config-kata.git"
  },
  "author": "Resi Respati <resi@kata.ai>",
