  },
  "dependencies": {
    "tslint-config-prettier": "^1.10.0",
    "tslint-consistent-codestyle": "^1.13.0",
    "tslint-eslint-rules": "^5.1.0",
    "tslint-microsoft-contrib": "^5.0.3",
    "tslint-react": "^3.5.1"
  },
  "config": {
    "commitizen": {
